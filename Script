CMDX.BackgroundColor3 = Color3.fromRGB(37, 37, 37)
CMDX.Position = UDim2.new(0.0215439871, 0, 0.0188087747, 0)
CMDX.Size = UDim2.new(0, 180, 0, 50)
CMDX.Font = Enum.Font.TitilliumWeb
CMDX.Text = "CMD X"
CMDX.TextColor3 = Color3.fromRGB(255, 255, 255)
CMDX.TextSize = 50.000
CMDX.TextWrapped = true
CMDX.MouseButton1Down:connect(function()
